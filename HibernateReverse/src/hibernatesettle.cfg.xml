<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration SYSTEM "classpath://hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory name="settle">
        <property name="hibernate.connection.driver_class">org.gjt.mm.mysql.Driver</property>
        <property name="hibernate.connection.password">123456</property>
        <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/mts_settle</property>
        <property name="hibernate.connection.username">root</property>
        <property name="hibernate.default_catalog">mts_settle</property>
        <property name="hibernate.default_schema">mts_settle</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLInnoDBDialect</property>
    </session-factory>
</hibernate-configuration>
